<h3>Register</h3>

<%= form_for :user, url: '/users' do |f| %>

  <%= render "errors" %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username%>
  </p>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit "Sign up" %>
  </p>
<% end %>